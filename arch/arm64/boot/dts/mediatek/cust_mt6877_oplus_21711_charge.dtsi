&i2c10 {
	i2c_reset_bus = <10>;
	pinctrl-names = "i2c-state", "output-low-state";
	pinctrl-0 = <&i2c10_i2c_state>;
	pinctrl-1 = <&i2c10_output_low_state>;
};
&pio {
	i2c10_i2c_state: i2c10_sda_i2c_state {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO192__FUNC_SDA10>;
			slew-rate = <0>;
			bias-disable;
		};
	};
	i2c10_output_low_state: i2c10_sda_output_low_state {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO192__FUNC_GPIO192>;
			slew-rate = <1>;
			output-low;
		};
	};
};

&i2c10 {
	status = "okay";
