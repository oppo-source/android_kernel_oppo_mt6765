    };

        linktem_disable: linktem_disable {
        pins_cmd_dat {
            pinmux = <PINMUX_GPIO41__FUNC_GPIO41>;
            slew-rate = <1>;//0:input direction 1:output direction
            bias-pull-up = <00>;
