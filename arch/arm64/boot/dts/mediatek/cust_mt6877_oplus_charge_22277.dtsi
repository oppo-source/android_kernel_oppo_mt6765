&odm {
	mediatek_phy_tuning: mediatek_phy_tuning {
		phy_tuning {
			compatible = "mediatek,phy_tuning";
			eye-vrt = <6>;
			eye-term = <6>;
			eye-rev6 = <3>;
			u2_discth = <0xc>;
			host-eye-vrt = <6>;
			host-eye-term = <6>;
			host-eye-rev6 = <3>;
			host_u2_discth = <0xc>;
		};
	};

