&kd_camera_hw1 {
	pinctrl-names = "default",
			"cam0_rst0", "cam0_rst1",
			"cam0_pnd0", "cam0_pnd1",
			"cam1_rst0", "cam1_rst1",
			"cam1_pnd0", "cam1_pnd1",
			"cam1_ldo_vcama1_0", "cam1_ldo_vcama1_1",
			"cam2_pnd0", "cam2_pnd1",
			"cam2_ldo_vcamd_0", "cam2_ldo_vcamd_1",
			"cam2_rst0", "cam2_rst1",
			"cam3_rst0", "cam3_rst1",

			"cam0_mclk_off",
			"cam0_mclk_2mA", "cam0_mclk_4mA",
			"cam0_mclk_6mA", "cam0_mclk_8mA",
			"cam1_mclk_off",
			"cam1_mclk_2mA", "cam1_mclk_4mA",
			"cam1_mclk_6mA", "cam1_mclk_8mA",
			"cam2_mclk_off",
			"cam2_mclk_2mA", "cam2_mclk_4mA",
			"cam2_mclk_6mA", "cam2_mclk_8mA",
			"cam3_mclk_off",
			"cam3_mclk_2mA", "cam3_mclk_4mA",
			"cam3_mclk_6mA", "cam3_mclk_8mA",

			"fan53870_chip_enable";

	pinctrl-0 = <&camera_pins_default>;

	pinctrl-1 = <&camera_pins_cam0_rst_0>;
	pinctrl-2 = <&camera_pins_cam0_rst_1>;
	pinctrl-3 = <&camera_pins_cam0_pnd_0>;
	pinctrl-4 = <&camera_pins_cam0_pnd_1>;
	pinctrl-5 = <&camera_pins_cam1_rst_0>;
	pinctrl-6 = <&camera_pins_cam1_rst_1>;
	pinctrl-7 = <&camera_pins_cam1_pnd_0>;
	pinctrl-8 = <&camera_pins_cam1_pnd_1>;
	pinctrl-9 = <&camera_pins_cam1_vcama1_0>;
	pinctrl-10 = <&camera_pins_cam1_vcama1_1>;
	pinctrl-11 = <&camera_pins_cam2_pnd_0>;
	pinctrl-12 = <&camera_pins_cam2_pnd_1>;
	pinctrl-13 = <&camera_pins_cam2_vcamd_0>;
	pinctrl-14 = <&camera_pins_cam2_vcamd_1>;
	pinctrl-15 = <&camera_pins_cam2_rst_0>;
	pinctrl-16 = <&camera_pins_cam2_rst_1>;
	pinctrl-17 = <&camera_pins_cam3_rst_0>;
	pinctrl-18 = <&camera_pins_cam3_rst_1>;

	pinctrl-19 = <&camera_pins_cam0_mclk_off>;
	pinctrl-20 = <&camera_pins_cam0_mclk_2ma>;
	pinctrl-21 = <&camera_pins_cam0_mclk_4ma>;
	pinctrl-22 = <&camera_pins_cam0_mclk_6ma>;
	pinctrl-23 = <&camera_pins_cam0_mclk_8ma>;
	pinctrl-24 = <&camera_pins_cam1_mclk_off>;
	pinctrl-25 = <&camera_pins_cam1_mclk_2ma>;
	pinctrl-26 = <&camera_pins_cam1_mclk_4ma>;
	pinctrl-27 = <&camera_pins_cam1_mclk_6ma>;
	pinctrl-28 = <&camera_pins_cam1_mclk_8ma>;
	pinctrl-29 = <&camera_pins_cam2_mclk_off>;
	pinctrl-30 = <&camera_pins_cam2_mclk_2ma>;
	pinctrl-31 = <&camera_pins_cam2_mclk_4ma>;
	pinctrl-32 = <&camera_pins_cam2_mclk_6ma>;
	pinctrl-33 = <&camera_pins_cam2_mclk_8ma>;
	pinctrl-34 = <&camera_pins_cam3_mclk_off>;
	pinctrl-35 = <&camera_pins_cam3_mclk_2ma>;
	pinctrl-36 = <&camera_pins_cam3_mclk_4ma>;
	pinctrl-37 = <&camera_pins_cam3_mclk_6ma>;
	pinctrl-38 = <&camera_pins_cam3_mclk_8ma>;

	pinctrl-39 = <&fan53870_gpio_enable>;

	cam0_vcamio-supply = <&mt_pmic_vcamio_ldo_reg>;
	cam1_vcamio-supply = <&mt_pmic_vcamio_ldo_reg>;
	cam2_vcamio-supply = <&mt_pmic_vcamio_ldo_reg>;
	cam3_vcamio-supply = <&mt_pmic_vcamio_ldo_reg>;

	cam0_enable_sensor = "imx766_mipi_raw_21305";
	cam1_enable_sensor = "imx709_mipi_raw_21305";
	cam2_enable_sensor = "imx355_mipi_raw_21305";
	cam3_enable_sensor = "gc02m1_mipi_raw_21305";

	status = "okay";
};

/* CAMERA GPIO end */
&i2c8 {
        fan53870@35 {
                compatible = "fan53870";
                reg = <0x35>;
                ldo1_min_vol = <800000>;  /*uV*/
                ldo1_max_vol = <1504000>;
                ldo1_step_vol = <8000>;
                en-gpios = <&pio 29 1>;
        };
};

/* CAMERA EEPROM */
&i2c2 {
	mtk_camera_eeprom0:camera_eeprom0@51 {
		compatible = "mediatek,camera_eeprom";
		reg = <0x51>;
		status = "okay";
	};
};
&i2c4 {
