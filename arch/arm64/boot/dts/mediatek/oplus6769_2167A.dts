	pinctrl-1 = <&chiponefp_rst_high>;
	pinctrl-2 = <&chiponefp_irq>;
	pinctrl-3 = <&chiponefp_power_high>;
	pinctrl-4 = <&chiponefp_power_low>;
	status = "okay";
};
&pio {
	chiponefp_rst_low: chiponefp_rst_low {
